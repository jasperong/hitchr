<div>
  <%= form_for([@user, @review]) do |f| %>
    <div class="rateYo">
      <input type="hidden" name="rating" value="" />
    </div>
    <div class="field">
      <%= f.label :review %>
      <%= f.text_field :review %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <%end%>
</div>
